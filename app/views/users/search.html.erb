<% if logged_in? %>
  <div class="opendysfont">
    <nav>
      <div class="nav-wrapper">
        <ul class="right">
          <li><a href="/users"><i class="material-icons left">face</i>Preferences</a></li>
          <li><a href="/logout"><i class="material-icons left">exit_to_app</i>Logout</a></li>
        </ul>
      </div>
    </nav>

    <div class="container">
      <form class="col s6 offset-s6" action="/users/url" method="post">
        <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

        <input id="web-search" type="text" name="url">
        <button id="search-btn" class="btn">Search</button>
      </form>
    </div>
  </div>
<% else %>

<h1>Sorry not logged in</h1>

<% end %>
